neg.LL.function.for.optim <- function(par, dat)
{
x <- dat$x
y <- dat$y
a <- par[1]
b <- par[2]
sd <- par[3]
LL <- dnorm(y, mean=a + b*x, sd=sd, log=TRUE)
neg.LL <- - sum(LL)
return(neg.LL)
}
optim(par=c(a=0, b=0, sd=1),
fn=neg.LL.function.for.optim,
dat=catepil)
