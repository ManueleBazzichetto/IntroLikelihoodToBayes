neg.LL.function.for.optim <- function(par, dat)
{
tannin <- dat$tannin
growth <- dat$growth
a <- par[1]
b <- par[2]
sd <- par[3]
# likelihood
mu <- a + b*tannin
LL <- dnorm(growth, mean=mu, sd=sd)
# negative log-likelihood
neg.LL <- - sum(log(LL))
return(neg.LL)
}
